{{ define "enums" }}

{{- range $lang, $enums := . }}
<div>
<pulumi-choosable type="language" values="{{ print $lang }}">
<dl class="tabular">
{{- range . -}}
    <dt>{{- markdownify .DisplayName -}}</dt>
    <dd>{{- markdownify .Value }}{{- if .Comment }}{{- markdownify .Comment -}}{{ end -}}{{- if .DeprecationMessage }}<p class="property-message">Deprecated: {{- markdownify .DeprecationMessage -}}</p>{{- end -}}</dd>
{{- end -}}
</dl>
</pulumi-choosable>
</div>
{{- end }}
{{- end }}
